{
  "name": "revenge-phaser",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run pre && run-p watch live",
    "build": "run-s pre package deploy",
    "package": "webpack",
    "watch": "webpack --watch",
    "live": "live-server",
    "nw": "nw ./export/src",
    "....................": "",
    "pre": "run-s pre:*",
    "pre:mkdirp": "shx mkdir -p ./lib",
    "pre:engine": "shx cp ./node_modules/phaser/dist/phaser-arcade-physics.min.js ./lib/engine.js",
    ",...................": "",
    "deploy": "run-s deploy:*",
    "deploy:rm": "shx rm -rf ./export",
    "deploy:cp-root": "cpx ./index.html ./export/src",
    "deploy:cp-meta": "cpx \"./{assets,lib}/**/*.*\" ./export/src",
    "deploy:cp-nw": "cpx ./nw.json ./export/src && shx mv ./export/src/nw.json ./export/src/package.json",
    "deploy:web-zip": "bestzip ./export/web.zip ./export/src",
    "deploy:nw": "nwbuild --platforms win32,win64,osx64,linux32,linux64 --buildDir ./export/dist ./export/src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katai5plate/revenge-phaser.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/katai5plate/revenge-phaser/issues"
  },
  "homepage": "https://github.com/katai5plate/revenge-phaser#readme",
  "devDependencies": {
    "bestzip": "^2.1.4",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.1",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "nw": "^0.40.2",
    "nw-builder": "^3.5.7",
    "phaser": "^3.19.0",
    "shx": "^0.3.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.6.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8"
  }
}
